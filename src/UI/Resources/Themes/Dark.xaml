<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush x:Key="Primary" Color="#40BAFF" />
    <SolidColorBrush x:Key="Secondary" Color="#40BAFF" />
    <SolidColorBrush x:Key="Green" Color="#54D62C" />
    <SolidColorBrush x:Key="Red" Color="#FF4842" />
    <SolidColorBrush x:Key="Text" Color="#ffffff" />
    <SolidColorBrush x:Key="ButtonText" Color="#ffffff" />
    <SolidColorBrush x:Key="MainBackground" Color="#222426" />
    <SolidColorBrush x:Key="CenterColumnBackground" Color="#282E39" />
    <SolidColorBrush x:Key="RightColumnBackground" Color="#252B36" />
    <SolidColorBrush x:Key="InputBackground" Color="#454F5B" />
    
    <SolidColorBrush x:Key="Gray100" Color="#161C24" />
    <SolidColorBrush x:Key="Gray200" Color="#212B36" />
    <SolidColorBrush x:Key="Gray300" Color="#454F5B" />
    <SolidColorBrush x:Key="Gray400" Color="#637381" />
    <SolidColorBrush x:Key="Gray500" Color="#95A4AB" />
    <SolidColorBrush x:Key="Gray600" Color="#C4CDD5" />
    <SolidColorBrush x:Key="Gray700" Color="#DFE3E8" />
    <SolidColorBrush x:Key="Gray800" Color="#F4F6F8" />
    <SolidColorBrush x:Key="Gray900" Color="#F9FAFB" />
    
    <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="#ffffff"/>
    <SolidColorBrush x:Key="{x:Static SystemColors.HighlightTextBrushKey}" Color="#ffffff"/>
</ResourceDictionary>